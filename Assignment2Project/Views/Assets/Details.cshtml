@model Assignment2Project.Models.AssetsModel

@{
    ViewData["Title"] = "Details";
}

<h1>Asset Details</h1>

<div>
 
    <dl class="row mt-3">
        <dt class="col-sm-2">
            <a>Assest Name</a>
<dd class="col-sm-10">
